    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.2.3",
    "glob": "^11.0.0",
    "globals": "^16.0.0",
